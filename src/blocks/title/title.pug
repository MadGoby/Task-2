mixin title(settings)
  - let {
  -   text = "For change",
  -   teg = "p",
  -   type = false,
  -   jsClass = false,
  -   length = false,
  -   for: targetClass = false
  - } = settings
  - if (type) type = `title_${type}`
  - if (targetClass) targetClass = `title_for-${targetClass}`
  - if (length) length = `title_length_${length}`
  #{teg}(class="title", class=type,  class=targetClass, class=jsClass, class=length)=text